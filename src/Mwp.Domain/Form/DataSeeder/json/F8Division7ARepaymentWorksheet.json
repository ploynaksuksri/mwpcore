      "content": "function f8FuncCalculateMinRepayTotal(openingBalance, rateInPct, yearsUntilLoanTerminates) {\r\n\tvar rate = rateInPct / 100;\r\n\tvar total = (openingBalance * rate) / (1 - (1 / Math.pow(1 + rate, yearsUntilLoanTerminates)));\r\n\treturn Math.round(total * 100) / 100;\r\n}\r\n\r\nfunction f8FuncCalculateMinRepayInterest(openingBalance, rateInPct) {\r\n\tvar rate = rateInPct / 100;\r\n\tvar interestAmount = rate * openingBalance;\r\n\treturn Math.round(interestAmount * 100) / 100;\r\n}\r\n\r\nfunction f8FuncCalculateMinRepayPrincipal(openingBalance, rateInPct, yearsUntilLoanTerminates) {\r\n\tvar total = f8FuncCalculateMinRepayTotal(openingBalance, rateInPct, yearsUntilLoanTerminates);\r\n\tvar interestAmount = f8FuncCalculateMinRepayInterest(openingBalance, rateInPct);\r\n\treturn total - interestAmount;\r\n}\r\n\r\nfunction f8FuncCalculateLoanClosingBalance(openingBalance, rateInPct, repayAmount){\r\n\tvar interestAmount = f8FuncCalculateMinRepayInterest(openingBalance, rateInPct);\r\n\treturn openingBalance + interestAmount - repayAmount;\r\n}\r\n\r\nfunction f8FuncIsMinRepayMade(openingBalance, rateInPct, yearsUntilLoanTerminates, repayAmount){\r\n\tvar total = f8FuncCalculateMinRepayTotal(openingBalance, rateInPct, yearsUntilLoanTerminates);\r\n\tif(total > repayAmount){\r\n\t\treturn 'NO';\r\n\t} else {\r\n\t\treturn 'YES';\r\n\t}\r\n}",
